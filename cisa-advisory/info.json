{
    "name": "cisa-advisory",
    "version": "1.0.0",
    "label": "CISA Advisory",
    "description": "CISA Advisory connector fetches the Advisory published by CISA",
    "publisher": "Fortinet",
    "icon_small_name": "samll.png",
    "icon_large_name": "large.png",
    "configuration": {
        "fields": [
            {
                "editable": true,
                "name": "prodYumRepoURL",
                "required": true,
                "title": "Prod Yum Repo URL",
                "type": "text",
                "value": "repo.fortisoar.fortinet.com",
                "visible": true,
                "description": "Specify the URL of the production repository that hosts the CISA Advisory Data."
            }
        ]
    },
    "category": "Utilities",
    "active": true,
    "cs_compatible": true,
    "help_online": "https://docs.fortinet.com/document/fortisoar/1.0.0/cisa-advisory/614/cisa-advisory-v1-0-0",
    "cs_approved": false,
    "operations": [
        {
            "operation": "get_advisory",
            "title": "Get Advisory",
            "annotation": "get_advisory",
            "description": "Fetches all the advisories published by CISA up to the current date and time, or alternatively, the date and time you specify, based on the selected advisory type.",
            "category": "investigation",
            "is_config_required": true,
            "visible": true,
            "enabled": true,
            "parameters": [
                {
                    "name": "advisory_type",
                    "title": "Advisory Type",
                    "type": "select",
                    "editable": true,
                    "visible": true,
                    "required": true,
                    "description": "Select the type of advisory that you want to fetch from CISA.",
                    "options": [
                        "ICS Advisory",
                        "ICS Medical Advisory"
                    ],
                    "tooltip": "Select the type of advisory that you want to fetch from CISA."
                },
                {
                    "name": "date_filter",
                    "title": "Date Filter",
                    "type": "select",
                    "editable": true,
                    "visible": true,
                    "required": false,
                    "description": "Select the date and time till when you want to fetch advisories from CISA.",
                    "options": [
                        "Last 7 Days",
                        "Last 30 Days",
                        "This Month",
                        "This Year",
                        "Last Month",
                        "Last 3 Months",
                        "Last 6 Months",
                        "Last Date Published Advisory",
                        "Custom Date"
                    ],
                    "onchange": {
                        "Custom Date": [
                            {
                                "name": "selectDate",
                                "title": "Select Date",
                                "description": "Fetches all the advisory by given date.",
                                "type": "datetime",
                                "editable": true,
                                "visible": true,
                                "required": true,
                                "value": "",
                                "tooltip": "Specify the release date or epoch time of advisory."
                            }
                        ]
                    },
                    "tooltip": "Select the date and time till when you want to fetch advisories from CISA."
                }
            ],
            "output_schema": {
                "url": "",
                "name": "",
                "icsa_code": "",
                "year": "",
                "vendor": "",
                "product": "",
                "cve_list": [],
                "cwe_list": [],
                "release_date": "",
                "updated_date": "",
                "cvss_severity": "",
                "cvss_score": "",
                "affected_product": "",
                "company_headquarters_location": "",
                "country_area_deployed": "",
                "critical_infrastructure_sector": "",
                "tag_list": []
            }
        },
        {
            "operation": "get_advisory_by_year",
            "title": "Get Advisory By Year",
            "annotation": "get_advisory_by_year",
            "description": "Fetches all the advisories published by CISA based on the specified year and selected advisory type.",
            "category": "investigation",
            "is_config_required": true,
            "visible": true,
            "enabled": true,
            "parameters": [
                {
                    "name": "advisory_type",
                    "title": "Advisory Type",
                    "type": "select",
                    "editable": true,
                    "visible": true,
                    "required": true,
                    "description": "Select the type of advisory that you want to fetch from CISA.",
                    "options": [
                        "ICS Advisory",
                        "ICS Medical Advisory"
                    ],
                    "tooltip": "Select the type of advisory that you want to fetch from CISA.",
                    "onchange": {
                        "ICS Advisory": [
                            {
                                "title": "Year",
                                "type": "select",
                                "name": "year",
                                "required": true,
                                "visible": true,
                                "editable": true,
                                "options": [
                                    "2023",
                                    "2022",
                                    "2021",
                                    "2020",
                                    "2019",
                                    "2018",
                                    "2017",
                                    "2016",
                                    "2015",
                                    "2014",
                                    "2013",
                                    "2012",
                                    "2011",
                                    "2010"
                                ],
                                "tooltip": "Select the year for which you want to fetch the advisories from CISA."
                            }
                        ],
                        "ICS Medical Advisory": [
                            {
                                "title": "Year",
                                "type": "select",
                                "name": "year",
                                "required": true,
                                "visible": true,
                                "editable": true,
                                "options": [
                                    "2023",
                                    "2022",
                                    "2021",
                                    "2020",
                                    "2019",
                                    "2018",
                                    "2017",
                                    "2016"
                                ],
                                "tooltip": "Select the year for which you want to fetch the advisories from CISA."
                            }
                        ]
                    }
                }
            ],
            "output_schema": {
                "url": "",
                "name": "",
                "icsa_code": "",
                "year": "",
                "vendor": "",
                "product": "",
                "cve_list": [],
                "cwe_list": [],
                "release_date": "",
                "updated_date": "",
                "cvss_severity": "",
                "cvss_score": "",
                "affected_product": "",
                "company_headquarters_location": "",
                "country_area_deployed": "",
                "critical_infrastructure_sector": "",
                "tag_list": []
            }
        },
        {
            "operation": "get_advisory_by_vendor",
            "title": "Get Advisory By Vendor",
            "annotation": "get_advisory_by_vendor",
            "description": "Fetches all the advisories published by CISA based on the specified vendor and selected advisory type. ",
            "category": "investigation",
            "is_config_required": true,
            "visible": true,
            "enabled": true,
            "parameters": [
                {
                    "name": "advisory_type",
                    "title": "Advisory Type",
                    "type": "select",
                    "editable": true,
                    "visible": true,
                    "required": true,
                    "description": "Select the type of advisory that you want to fetch from CISA. ",
                    "options": [
                        "ICS Advisory",
                        "ICS Medical Advisory"
                    ],
                    "tooltip": "Select the type of advisory that you want to fetch from CISA. "
                },
                {
                    "title": "Vendor",
                    "type": "text",
                    "name": "vendor",
                    "required": true,
                    "visible": true,
                    "editable": true,
                    "tooltip": "Specify the vendor whose associated advisory you want to fetch from CISA."
                },
                {
                    "title": "Similarity Threshold",
                    "type": "integer",
                    "name": "similarityThreshold",
                    "required": true,
                    "visible": true,
                    "editable": true,
                    "value": 75,
                    "tooltip": "Specify the similarity threshold (percentage) that will be used to compare vendors with the specified vendor when retrieving the advisories from CISA."
                }
            ],
            "output_schema": {
                "url": "",
                "name": "",
                "icsa_code": "",
                "year": "",
                "vendor": "",
                "product": "",
                "cve_list": [],
                "cwe_list": [],
                "release_date": "",
                "updated_date": "",
                "cvss_severity": "",
                "cvss_score": "",
                "affected_product": "",
                "company_headquarters_location": "",
                "country_area_deployed": "",
                "critical_infrastructure_sector": "",
                "tag_list": []
            }
        },
        {
            "operation": "get_advisory_by_product",
            "title": "Get Advisory By Product",
            "annotation": "get_advisory_by_product",
            "description": "Fetches all the advisories published by CISA based on the specified vendor, its product, its version, and selected advisory type. ",
            "category": "investigation",
            "is_config_required": true,
            "visible": true,
            "enabled": true,
            "parameters": [
                {
                    "name": "advisory_type",
                    "title": "Advisory Type",
                    "type": "select",
                    "editable": true,
                    "visible": true,
                    "required": true,
                    "description": "Select the type of advisory that you want to fetch from CISA.",
                    "options": [
                        "ICS Advisory",
                        "ICS Medical Advisory"
                    ],
                    "tooltip": "Select the type of advisory that you want to fetch from CISA."
                },
                {
                    "title": "Vendor",
                    "type": "text",
                    "name": "vendor",
                    "required": true,
                    "visible": true,
                    "editable": true,
                    "tooltip": "Specify the vendor whose associated advisory you want to fetch from CISA."
                },
                {
                    "title": "Product",
                    "type": "text",
                    "name": "product",
                    "required": true,
                    "visible": true,
                    "editable": true,
                    "tooltip": "Specify the product whose associated advisory you want to fetch from CISA."
                },
                {
                    "title": "Version",
                    "type": "text",
                    "name": "version",
                    "required": false,
                    "visible": true,
                    "editable": true,
                    "tooltip": "Specify the version of the specified product whose associated advisory you want to fetch from CISA."
                },
                {
                    "title": "Vendor Similarity Threshold",
                    "type": "integer",
                    "name": "vendorSimilarityThreshold",
                    "required": true,
                    "visible": true,
                    "editable": true,
                    "value": 75,
                    "tooltip": "Specify the similarity threshold (percentage) that will be used to compare vendors with the specified vendor when retrieving the advisories from CISA."
                },
                {
                    "title": "Product Similarity Threshold",
                    "type": "integer",
                    "name": "productSimilarityThreshold",
                    "required": true,
                    "visible": true,
                    "editable": true,
                    "value": 75,
                    "tooltip": "Specify the similarity threshold (percentage) that will be used to compare products with the specified product when retrieving the advisories from CISA. "
                }
            ],
            "output_schema": {
                "url": "",
                "name": "",
                "icsa_code": "",
                "year": "",
                "vendor": "",
                "product": "",
                "cve_list": [],
                "cwe_list": [],
                "release_date": "",
                "updated_date": "",
                "cvss_severity": "",
                "cvss_score": "",
                "affected_product": "",
                "company_headquarters_location": "",
                "country_area_deployed": "",
                "critical_infrastructure_sector": "",
                "tag_list": []
            }
        }
    ]
}